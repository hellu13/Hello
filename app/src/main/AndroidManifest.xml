<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.sliverapp">

    <uses-permission android:name="android.permission.CAMERA" /> <!-- 카메라 권한 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> <!-- 저장소 읽기 권한 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" /> <!-- Conteact 권한 -->
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.techtown.sliverapp.AlarmEActivity"></activity>
        <activity android:name="org.techtown.sliverapp.AlarmLActivity" />
        <activity android:name="org.techtown.sliverapp.AlarmMActivity" />
        <activity android:name="org.techtown.sliverapp.AlarmActivity" />
        <activity android:name="org.techtown.sliverapp.NoteDetailActivity" />
        <activity android:name="org.techtown.sliverapp.SettingsActivity" />
        <activity android:name="org.techtown.sliverapp.AppPreferences" />
        <activity android:name="org.techtown.sliverapp.ZoomActivity" />
        <activity
            android:name="org.techtown.sliverapp.CallingActivity"
            android:label="전화걸기 액티비티"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
        android:name="org.techtown.sliverapp.CallingFActivity"
        android:label="가족 전화걸기 액티비티"
        android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name="org.techtown.sliverapp.CallingRActivity"
            android:label="친척 전화걸기 액티비티"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity android:name="org.techtown.sliverapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name="org.techtown.sliverapp.Alarm" />
    </application>

</manifest>